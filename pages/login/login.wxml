<!--login.wxml-->
<view class="main">
  <!--标题链接-->
  <view>
     <navigator url="../index/index" open-type="navigate" class="uploader-text">
      <text class="titlem">starts管理系统</text>
     </navigator>
  </view>
  <view class="circle">
      <image class='header-bg' src = '../../images/123.png' mode="aspectFit"></image>
      <view class="username box">
          <input type="text" class="input1" name="userName" id="userName" value="" placeholder="username" />
      </view>
      <view class="password box">
          <input type="text" class="input1" name="password" id="password" value="" placeholder="password" />
      </view>
      <view class="login box">
          <button style="width:50px;height:50px;font-size:30rpx;margin: 0 10rpx;" type="button" bindtap="login">登录</button>
          <button style="width:50px;height:50px;font-size:30rpx;" type="button" bindtap="register">注册</button>
      </view>
  </view>


</view>
